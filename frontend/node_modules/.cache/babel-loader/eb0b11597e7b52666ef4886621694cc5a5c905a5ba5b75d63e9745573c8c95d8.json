\" d=\"M18.685 19.097A9.723 9.723 0 0021.75 12c0-5.385-4.365-9.75-9.75-9.75S2.25 6.615 2.25 12a9.723 9.723 0 003.065 7.097A9.716 9.716 0 0012 21.75a9.716 9.716 0 006.685-2.653zm-12.54-1.285A7.486 7.486 0 0112 15a7.486 7.486 0 015.855 2.812A8.224 8.224 0 0112 20.25a8.224 8.224 0 01-5.855-2.438zM15.75 9a3.75 3.75 0 11-7.5 0 3.75 3.75 0 017.5 0z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </div>\r\n            <input\r\n              id=\"signupName\"\r\n              v-model=\"signupData.name\"\r\n              type=\"text\"\r\n              placeholder=\"Enter your full name\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label for=\"signupUsername\">Username</label>\r\n          <div class=\"input-container\">\r\n            <div class=\"input-icon\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path fill-rule=\"evenodd\" d=\"M7.5 6a4.5 4.5 0 119 0 4.5 4.5 0 01-9 0zM3.751 20.105a8.25 8.25 0 0116.498 0 .75.75 0 01-.437.695A18.683 18.683 0 0112 22.5c-2.786 0-5.433-.608-7.812-1.7a.75.75 0 01-.437-.695z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </div>\r\n            <input\r\n              id=\"signupUsername\"\r\n              v-model=\"signupData.username\"\r\n              type=\"text\"\r\n              placeholder=\"Choose a username\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label for=\"signupPassword\">Password</label>\r\n          <div class=\"input-container\">\r\n            <div class=\"input-icon\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path fill-rule=\"evenodd\" d=\"M12 1.5a5.25 5.25 0 00-5.25 5.25v3a3 3 0 00-3 3v6.75a3 3 0 003 3h10.5a3 3 0 003-3v-6.75a3 3 0 00-3-3v-3c0-2.9-2.35-5.25-5.25-5.25zm3.75 8.25v-3a3.75 3.75 0 10-7.5 0v3h7.5z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </div>\r\n            <input\r\n              id=\"signupPassword\"\r\n              v-model=\"signupData.password\"\r\n              :type=\"showSignupPassword ? 'text' : 'password'\"\r\n              placeholder=\"Create a strong password\"\r\n              required\r\n            />\r\n            <button type=\"button\" class=\"password-toggle\" @click=\"showSignupPassword = !showSignupPassword\">\r\n              <svg v-if=\"showSignupPassword\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path d=\"M3.53 2.47a.75.75 0 00-1.06 1.06l18 18a.75.75 0 101.06-1.06l-18-18zM22.676 12.553a11.249 11.249 0 01-2.631 4.31l-3.099-3.099a5.25 5.25 0 00-6.71-6.71L7.759 4.577a11.217 11.217 0 014.242-.827c4.97 0 9.185 3.223 10.675 7.69.12.362.12.752 0 1.113z\" />\r\n                <path d=\"M15.75 12c0 .18-.013.357-.037.53l-4.244-4.243A3.75 3.75 0 0115.75 12zM12.53 15.713l-4.243-4.244a3.75 3.75 0 004.243 4.243z\" />\r\n                <path d=\"M6.75 12c0-.619.107-1.213.304-1.764l-3.1-3.1a11.25 11.25 0 00-2.63 4.31c-.12.362-.12.752 0 1.114 1.489 4.467 5.704 7.69 10.675 7.69 1.5 0 2.933-.294 4.242-.827l-2.477-2.477A5.25 5.25 0 016.75 12z\" />\r\n              </svg>\r\n              <svg v-else xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path d=\"M12 15a3 3 0 100-6 3 3 0 000 6z\" />\r\n                <path fill-rule=\"evenodd\" d=\"M1.323 11.447C2.811 6.976 7.028 4.75 11.999 4.75c4.97 0 9.185 2.223 10.675 6.696.123.377.123.754 0 1.131C21.185 17.024 16.97 19.25 12 19.25c-4.97 0-9.185-2.223-10.675-6.697a1.762 1.762 0 010-1.131zM17.25 12a5.25 5.25 0 11-10.5 0 5.25 5.25 0 0110.5 0z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </button>\r\n          </div>\r\n          <div class=\"password-hint\">\r\n            Password must be at least 8 characters with uppercase, lowercase, and numbers\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label for=\"confirmPassword\">Confirm Password</label>\r\n          <div class=\"input-container\">\r\n            <div class=\"input-icon\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path fill-rule=\"evenodd\" d=\"M12 1.5a5.25 5.25 0 00-5.25 5.25v3a3 3 0 00-3 3v6.75a3 3 0 003 3h10.5a3 3 0 003-3v-6.75a3 3 0 00-3-3v-3c0-2.9-2.35-5.25-5.25-5.25zm3.75 8.25v-3a3.75 3.75 0 10-7.5 0v3h7.5z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </div>\r\n            <input\r\n              id=\"confirmPassword\"\r\n              v-model=\"signupData.confirmPassword\"\r\n              :type=\"showConfirmPassword ? 'text' : 'password'\"\r\n              placeholder=\"Confirm your password\"\r\n              required\r\n            />\r\n            <button type=\"button\" class=\"password-toggle\" @click=\"showConfirmPassword = !showConfirmPassword\">\r\n              <svg v-if=\"showConfirmPassword\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path d=\"M3.53 2.47a.75.75 0 00-1.06 1.06l18 18a.75.75 0 101.06-1.06l-18-18zM22.676 12.553a11.249 11.249 0 01-2.631 4.31l-3.099-3.099a5.25 5.25 0 00-6.71-6.71L7.759 4.577a11.217 11.217 0 014.242-.827c4.97 0 9.185 3.223 10.675 7.69.12.362.12.752 0 1.113z\" />\r\n                <path d=\"M15.75 12c0 .18-.013.357-.037.53l-4.244-4.243A3.75 3.75 0 0115.75 12zM12.53 15.713l-4.243-4.244a3.75 3.75 0 004.243 4.243z\" />\r\n                <path d=\"M6.75 12c0-.619.107-1.213.304-1.764l-3.1-3.1a11.25 11.25 0 00-2.63 4.31c-.12.362-.12.752 0 1.114 1.489 4.467 5.704 7.69 10.675 7.69 1.5 0 2.933-.294 4.242-.827l-2.477-2.477A5.25 5.25 0 016.75 12z\" />\r\n              </svg>\r\n              <svg v-else xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path d=\"M12 15a3 3 0 100-6 3 3 0 000 6z\" />\r\n                <path fill-rule=\"evenodd\" d=\"M1.323 11.447C2.811 6.976 7.028 4.75 11.999 4.75c4.97 0 9.185 2.223 10.675 6.696.123.377.123.754 0 1.131C21.185 17.024 16.97 19.25 12 19.25c-4.97 0-9.185-2.223-10.675-6.697a1.762 1.762 0 010-1.131zM17.25 12a5.25 5.25 0 11-10.5 0 5.25 5.25 0 0110.5 0z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label class=\"role-label\">Register as:</label>\r\n          <div class=\"role-selection\">\r\n            <div \r\n              :class=\"['role-option', { selected: !signupData.isDoctor }]\"\r\n              @click=\"signupData.isDoctor = false\"\r\n            >\r\n              <div class=\"role-icon patient\">\r\n                <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                  <path d=\"M4.5 6.375a4.125 4.125 0 118.25 0 4.125 4.125 0 01-8.25 0zM14.25 8.625a3.375 3.375 0 116.75 0 3.375 3.375 0 01-6.75 0zM1.5 19.125a7.125 7.125 0 0114.25 0v.003l-.001.119a.75.75 0 01-.363.63 13.067 13.067 0 01-6.761 1.873c-2.472 0-4.786-.684-6.76-1.873a.75.75 0 01-.364-.63l-.001-.122zM17.25 19.128l-.001.144a2.25 2.25 0 01-.233.96 10.088 10.088 0 005.06-1.01.75.75 0 00.42-.643 4.875 4.875 0 00-6.957-4.611 8.586 8.586 0 011.71 5.157v.003z\" />\r\n                </svg>\r\n              </div>\r\n              <div class=\"role-text\">\r\n                <h3>Patient</h3>\r\n                <p>Book appointments and consult doctors</p>\r\n              </div>\r\n            </div>\r\n            <div \r\n              :class=\"['role-option', { selected: signupData.isDoctor }]\"\r\n              @click=\"signupData.isDoctor = true\"\r\n            >\r\n              <div class=\"role-icon doctor\">\r\n                <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                  <path d=\"M11.7 2.805a.75.75 0 01.6 0A60.65 60.65 0 0122.83 8.72a.75.75 0 01-.231 1.337 49.949 49.949 0 00-9.902 3.912l-.003.002-.34.18a.75.75 0 01-.707 0l-.34-.18-.003-.002a49.95 49.95 0 00-9.903-3.912.75.75 0 01-.231-1.337A60.653 60.653 0 0111.7 2.805z\" />\r\n                  <path d=\"M13.06 15.473a48.45 48.45 0 017.666-3.282c.134 1.414.22 2.843.255 4.285a.75.75 0 01-.46.71 47.878 47.878 0 00-8.105 4.342.75.75 0 01-.832 0 47.877 47.877 0 00-8.104-4.342.75.75 0 01-.461-.71c.035-1.442.121-2.87.255-4.286A48.4 48.4 0 016 13.18v1.27a1.5 1.5 0 00-.14 2.508c-.09.38-.222.753-.397 1.11.452.213.901.434 1.346.661a6.729 6.729 0 00.551-1.608 1.5 1.5 0 00.14-2.67v-.645a48.549 48.549 0 013.44 1.668 2.25 2.25 0 002.12 0z\" />\r\n                  <path d=\"M4.462 19.462c.42-.419.753-.89.99-1.394.237-.504.377-1.038.417-1.58a.75.75 0 00-.709-.796c-.375-.026-.75.013-1.124.04a5.56 5.56 0 00-2.077.442.75.75 0 00-.564.78c.014.643.093 1.283.237 1.909.144.626.352 1.234.618 1.816.237.54.568 1.01.991 1.428a.75.75 0 001.06-1.06z\" />\r\n                </svg>\r\n              </div>\r\n              <div class=\"role-text\">\r\n                <h3>Doctor</h3>\r\n                <p>Provide consultations and manage patients</p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <button type=\"submit\" :disabled=\"isLoading\" class=\"submit-btn\">\r\n          <span v-if=\"isLoading\" class=\"spinner\"></span>\r\n          <span v-else>Create Account</span>\r\n        </button>\r\n\r\n        <div class=\"terms-agreement\">\r\n          <p>By signing up, you agree to our <a href=\"#\">Terms of Service</a> and <a href=\"#\">Privacy Policy</a></p>\r\n        </div>\r\n      </form>\r\n\r\n      <!-- Forgot Password Modal -->\r\n      <div v-if=\"showForgotPassword\" class=\"modal-overlay\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h2>Reset Password</h2>\r\n            <button class=\"close-modal\" @click=\"showForgotPassword = false\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path fill-rule=\"evenodd\" d=\"M5.47 5.47a.75.75 0 011.06 0L12 10.94l5.47-5.47a.75.75 0 111.06 1.06L13.06 12l5.47 5.47a.75.75 0 11-1.06 1.06L12 13.06l-5.47 5.47a.75.75 0 01-1.06-1.06L10.94 12 5.47 6.53a.75.75 0 010-1.06z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <p>Enter your email address and we'll send you instructions to reset your password.</p>\r\n            <div class=\"form-group\">\r\n              <label for=\"resetEmail\">Email Address</label>\r\n              <input\r\n                id=\"resetEmail\"\r\n                v-model=\"resetEmail\"\r\n                type=\"email\"\r\n                placeholder=\"Enter your registered email\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button class=\"btn-secondary\" @click=\"showForgotPassword = false\">Cancel</button>\r\n            <button class=\"btn-primary\" @click=\"handleResetPassword\">Send Reset Link</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Footer -->\r\n    <div class=\"auth-footer\">\r\n      <p>Â© 2023 MediConnect. All rights reserved.</p>\r\n      <p>Need help? <a href=\"#\">Contact Support</a></p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LoginPage',\r\n  data() {\r\n    return {\r\n      isLoginMode: true,\r\n      isLoading: false,\r\n      showForgotPassword: false,\r\n      showLoginPassword: false,\r\n      showSignupPassword: false,\r\n      showConfirmPassword: false,\r\n      resetEmail: '',\r\n      loginData: {\r\n        username: '',\r\n        password: ''\r\n      },\r\n      signupData: {\r\n        name: '',\r\n        username: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        isDoctor: false\r\n      },\r\n      message: {\r\n        text: '',\r\n        type: '' // 'success' or 'error'\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    toggleMode(isLogin) {\r\n      this.isLoginMode = isLogin\r\n      this.message.text = ''\r\n    },\r\n    \r\n    async handleLogin() {\r\n      // Validate inputs\r\n      if (!this.loginData.username || !this.loginData.password) {\r\n        this.showMessage('Please fill in all fields', 'error')\r\n        return\r\n      }\r\n\r\n      this.isLoading = true\r\n      this.message.text = ''\r\n\r\n      try {\r\n        // API call for login\r\n        const response = await fetch('https://your-api-url.com/api/login', {\r\n          method: 'POST',\r\n          headers: {\r\n            'Content-Type': 'application/json',\r\n          },\r\n          body: JSON.stringify({\r\n            username: this.loginData.username,\r\n            password: this.loginData.password\r\n          })\r\n        })\r\n\r\n        const data = await response.json()\r\n\r\n        if (response.ok) {\r\n          this.showMessage('Login successful!', 'success')\r\n          // Store token and user data (you can use Vuex or localStorage)\r\n          localStorage.setItem('authToken', data.token)\r\n          localStorage.setItem('userData', JSON.stringify(data.user))\r\n          \r\n          // Redirect based on user role\r\n          setTimeout(() => {\r\n            if (data.user.isDoctor) {\r\n              window.location.href = '/doctor-dashboard'\r\n            } else {\r\n              window.location.href = '/patient-dashboard'\r\n            }\r\n          }, 1000)\r\n        } else {\r\n          this.showMessage(data.message || 'Login failed. Please check your credentials.', 'error')\r\n        }\r\n      } catch (error) {\r\n        console.error('Login error:', error)\r\n        this.showMessage('Network error. Please try again.', 'error')\r\n      } finally {\r\n        this.isLoading = false\r\n      }\r\n    },\r\n\r\n    async handleSignup() {\r\n      // Validate inputs\r\n      if (!this.signupData.name || !this.signupData.username || \r\n          !this.signupData.password || !this.signupData.confirmPassword) {\r\n        this.showMessage('Please fill in all fields', 'error')\r\n        return\r\n      }\r\n\r\n      if (this.signupData.password !== this.signupData.confirmPassword) {\r\n        this.showMessage('Passwords do not match', 'error')\r\n        return\r\n      }\r\n\r\n      // Password strength validation\r\n      const passwordRegex = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d).{8,}$/\r\n      if (!passwordRegex.test(this.signupData.password)) {\r\n        this.showMessage('Password must be at least 8 characters with uppercase, lowercase, and numbers', 'error')\r\n        return\r\n      }\r\n\r\n      this.isLoading = true\r\n      this.message.text = ''\r\n\r\n      try {\r\n        // API call for signup\r\n        const response = await fetch('https://your-api-url.com/api/signup', {\r\n          method: 'POST',\r\n          headers: {\r\n            'Content-Type': 'application/json',\r\n          },\r\n          body: JSON.stringify({\r\n            name: this.signupData.name,\r\n            username: this.signupData.username,\r\n            password: this.signupData.password,\r\n            isDoctor: this.signupData.isDoctor\r\n          })\r\n        })\r\n\r\n        const data = await response.json()\r\n\r\n        if (response.ok) {\r\n          this.showMessage('Account created successfully! Please login.', 'success')\r\n          \r\n          // Switch to login mode after successful signup\r\n          setTimeout(() => {\r\n            this.isLoginMode = true\r\n            // Clear signup form\r\n            this.signupData = {\r\n              name: '',\r\n              username: '',\r\n              password: '',\r\n              confirmPassword: '',\r\n              isDoctor: false\r\n            }\r\n          }, 2000)\r\n        } else {\r\n          this.showMessage(data.message || 'Signup failed. Please try again.', 'error')\r\n        }\r\n      } catch (error) {\r\n        console.error('Signup error:', error)\r\n        this.showMessage('Network error. Please try again.', 'error')\r\n      } finally {\r\n        this.isLoading = false\r\n      }\r\n    },\r\n\r\n    async handleResetPassword() {\r\n      if (!this.resetEmail) {\r\n        this.showMessage('Please enter your email address', 'error')\r\n        return\r\n      }\r\n\r\n      try {\r\n        // API call for password reset\r\n        const response = await fetch('https://your-api-url.com/api/reset-password', {\r\n          method: 'POST',\r\n          headers: {\r\n            'Content-Type': 'application/json',\r\n          },\r\n          body: JSON.stringify({ email: this.resetEmail })\r\n        })\r\n\r\n        const data = await response.json()\r\n\r\n        if (response.ok) {\r\n          this.showMessage('Password reset instructions sent to your email.', 'success')\r\n          this.showForgotPassword = false\r\n          this.resetEmail = ''\r\n        } else {\r\n          this.showMessage(data.message || 'Failed to send reset email.', 'error')\r\n        }\r\n      } catch (error) {\r\n        console.error('Reset password error:', error)\r\n        this.showMessage('Network error. Please try again.', 'error')\r\n      }\r\n    },\r\n\r\n    showMessage(text, type) {\r\n      this.message.text = text\r\n      this.message.type = type\r\n      \r\n      // Clear message after 5 seconds\r\n      setTimeout(() => {\r\n        this.message.text = ''\r\n      }, 5000)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.auth-container {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-directio