{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      isLoginMode: true,\n      isLoading: false,\n      showForgotPassword: false,\n      showLoginPassword: false,\n      showSignupPassword: false,\n      showConfirmPassword: false,\n      resetEmail: '',\n      loginData: {\n        username: '',\n        password: ''\n      },\n      signupData: {\n        name: '',\n        username: '',\n        password: '',\n        confirmPassword: '',\n        isDoctor: false\n      },\n      message: {\n        text: '',\n        type: '' // 'success' or 'error'\n      }\n    };\n  },\n  methods: {\n    toggleMode(isLogin) {\n      this.isLoginMode = isLogin;\n      this.message.text = '';\n    },\n    async handleLogin() {\n      // Validate inputs\n      if (!this.loginData.username || !this.loginData.password) {\n        this.showMessage('Please fill in all fields', 'error');\n        return;\n      }\n      this.isLoading = true;\n      this.message.text = '';\n      try {\n        // API call for login\n        const response = await fetch('https://your-api-url.com/api/login', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            username: this.loginData.username,\n            password: this.loginData.password\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.showMessage('Login successful!', 'success');\n          // Store token and user data (you can use Vuex or localStorage)\n          localStorage.setItem('authToken', data.token);\n          localStorage.setItem('userData', JSON.stringify(data.user));\n\n          // Redirect based on user role\n          setTimeout(() => {\n            if (data.user.isDoctor) {\n              window.location.href = '/doctor-dashboard';\n            } else {\n              window.location.href = '/patient-dashboard';\n            }\n          }, 1000);\n        } else {\n          this.showMessage(data.message || 'Login failed. Please check your credentials.', 'error');\n        }\n      } catch (error) {\n        console.error('Login error:', error);\n        this.showMessage('Network error. Please try again.', 'error');\n      } finally {\n        this.isLoading = false;\n      }\n    },\n    async handleSignup() {\n      // Validate inputs\n      if (!this.signupData.name || !this.signupData.username || !this.signupData.password || !this.signupData.confirmPassword) {\n        this.showMessage('Please fill in all fields', 'error');\n        return;\n      }\n      if (this.signupData.password !== this.signupData.confirmPassword) {\n        this.showMessage('Passwords do not match', 'error');\n        return;\n      }\n\n      // Password strength validation\n      const passwordRegex = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d).{8,}$/;\n      if (!passwordRegex.test(this.signupData.password)) {\n        this.showMessage('Password must be at least 8 characters with uppercase, lowercase, and numbers', 'error');\n        return;\n      }\n      this.isLoading = true;\n      this.message.text = '';\n      try {\n        // API call for signup\n        const response = await fetch('https://your-api-url.com/api/signup', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            name: this.signupData.name,\n            username: this.signupData.username,\n            password: this.signupData.password,\n            isDoctor: this.signupData.isDoctor\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.showMessage('Account created successfully! Please login.', 'success');\n\n          // Switch to login mode after successful signup\n          setTimeout(() => {\n            this.isLoginMode = true;\n            // Clear signup form\n            this.signupData = {\n              name: '',\n              username: '',\n              password: '',\n              confirmPassword: '',\n              isDoctor: false\n            };\n          }, 2000);\n        } else {\n          this.showMessage(data.message || 'Signup failed. Please try again.', 'error');\n        }\n      } catch (error) {\n        console.error('Signup error:', error);\n        this.showMessage('Network error. Please try again.', 'error');\n      } finally {\n        this.isLoading = false;\n      }\n    },\n    async handleResetPassword() {\n      if (!this.resetEmail) {\n        this.showMessage('Please enter your email address', 'error');\n        return;\n      }\n      try {\n        // API call for password reset\n        const response = await fetch('https://your-api-url.com/api/reset-password', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            email: this.resetEmail\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.showMessage('Password reset instructions sent to your email.', 'success');\n          this.showForgotPassword = false;\n          this.resetEmail = '';\n        } else {\n          this.showMessage(data.message || 'Failed to send reset email.', 'error');\n        }\n      } catch (error) {\n        console.error('Reset password error:', error);\n        this.showMessage('Network error. Please try again.', 'error');\n      }\n    },\n    showMessage(text, type) {\n      this.message.text = text;\n      this.message.type = type;\n\n      // Clear message after 5 seconds\n      setTimeout(() => {\n        this.message.text = '';\n      }, 5000);\n    }\n  }\n};","map":{"version":3,"names":["name","data","isLoginMode","isLoading","showForgotPassword","showLoginPassword","showSignupPassword","showConfirmPassword","resetEmail","loginData","username","password","signupData","confirmPassword","isDoctor","message","text","type","methods","toggleMode","isLogin","handleLogin","showMessage","response","fetch","method","headers","body","JSON","stringify","json","ok","localStorage","setItem","token","user","setTimeout","window","location","href","error","console","handleSignup","passwordRegex","test","handleResetPassword","email"],"sources":["F:\\programming\\medical-consultant\\frontend\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"auth-container\">\r\n    <div class=\"auth-card\">\r\n      <!-- Header Section -->\r\n      <div class=\"auth-header\">\r\n        <div class=\"logo-container\">\r\n          <div class=\"logo-icon\">\r\n            <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n              <path d=\"M9 12.75L11.25 15 15 9.75M21 12a9 9 0 11-18 0 9 9 0 0118 0z\" />\r\n            </svg>\r\n          </div>\r\n          <h1 class=\"logo-text\">MediConnect</h1>\r\n        </div>\r\n        <p class=\"tagline\">Secure Medical Consultation Platform</p>\r\n      </div>\r\n\r\n      <!-- Toggle between Login and Signup -->\r\n      <div class=\"auth-toggle\">\r\n        <button \r\n          :class=\"['toggle-btn', { active: isLoginMode }]\" \r\n          @click=\"toggleMode(true)\"\r\n        >\r\n          Login\r\n        </button>\r\n        <button \r\n          :class=\"['toggle-btn', { active: !isLoginMode }]\" \r\n          @click=\"toggleMode(false)\"\r\n        >\r\n          Sign Up\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Success/Error Messages -->\r\n      <div v-if=\"message.text\" :class=\"['message', message.type]\">\r\n        {{ message.text }}\r\n      </div>\r\n\r\n      <!-- Login Form -->\r\n      <form v-if=\"isLoginMode\" @submit.prevent=\"handleLogin\" class=\"auth-form\">\r\n        <div class=\"form-group\">\r\n          <label for=\"loginUsername\">Username</label>\r\n          <div class=\"input-container\">\r\n            <div class=\"input-icon\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path fill-rule=\"evenodd\" d=\"M7.5 6a4.5 4.5 0 119 0 4.5 4.5 0 01-9 0zM3.751 20.105a8.25 8.25 0 0116.498 0 .75.75 0 01-.437.695A18.683 18.683 0 0112 22.5c-2.786 0-5.433-.608-7.812-1.7a.75.75 0 01-.437-.695z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </div>\r\n            <input\r\n              id=\"loginUsername\"\r\n              v-model=\"loginData.username\"\r\n              type=\"text\"\r\n              placeholder=\"Enter your username\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label for=\"loginPassword\">Password</label>\r\n          <div class=\"input-container\">\r\n            <div class=\"input-icon\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path fill-rule=\"evenodd\" d=\"M12 1.5a5.25 5.25 0 00-5.25 5.25v3a3 3 0 00-3 3v6.75a3 3 0 003 3h10.5a3 3 0 003-3v-6.75a3 3 0 00-3-3v-3c0-2.9-2.35-5.25-5.25-5.25zm3.75 8.25v-3a3.75 3.75 0 10-7.5 0v3h7.5z\" clip-rule=\"evenodd\" />\r\n              </svg>\r\n            </div>\r\n            <input\r\n              id=\"loginPassword\"\r\n              v-model=\"loginData.password\"\r\n              :type=\"showLoginPassword ? 'text' : 'password'\"\r\n              placeholder=\"Enter your password\"\r\n              required\r\n            />\r\n            <button type=\"button\" class=\"password-toggle\" @click=\"showLoginPassword = !showLoginPassword\">\r\n              <svg v-if=\"showLoginPassword\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\r\n                <path d=\"M3.53 2.47a.75.75 0 00-1.06 1.06l18 18a.75.75 0 101.06-1.06l-18-18zM22.676 12.